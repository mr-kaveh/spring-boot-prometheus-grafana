{
  "id": null,
  "uid": "spring-dashboard",
  "title": "Spring Boot Metrics",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "JVM Memory Used",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes",
          "legendFormat": "{{area}} - {{id}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Custom Random Metric",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "custom_random_metric",
          "legendFormat": "Random Value",
          "refId": "B"
        }
      ]
    }
  ]
}
